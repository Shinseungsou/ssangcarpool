<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jfsiot.mju.ssangcarpool">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".SsangcarpoolApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/api_client_key" />
        <activity android:name=".activity.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainMapActivity">
        </activity>
        <activity android:name=".activity.SignUpActivity">
        </activity>
        <activity android:name=".activity.NavigationActivity"/>
        <activity android:name=".activity2.ListUserActivity"/>
        <activity android:name=".activity.HistoryDetailActivity"/>

        <activity android:name=".activity2.RiderActivity"/>
        <activity android:name=".activity2.MainActivity"/>
        <activity android:name=".activity2.MainActivity_h"/>
        <activity android:name=".activity2.SignUpActivity"/>
        <activity android:name=".activity.MessageActivity"/>
        <activity android:name=".activity2.PassengerActivity"/>
        <activity android:name=".activity2.ProfileActivity"/>
    </application>

</manifest>
